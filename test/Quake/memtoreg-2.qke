func.func @classical_if05(%q0: !quake.ref, %c1: i1) {
  cc.if (%c1) {
    cc.scope {
      %q1 = quake.alloca !quake.ref
      quake.x [%q0] %q1 : (!quake.ref, !quake.ref) -> ()
      quake.dealloc %q1 : !quake.ref
    }
    cc.continue
  }
  quake.y %q0 : (!quake.ref) -> ()
  return
}

