// CHECK:             cc.continue %[[VAL_5]]#0, %[[VAL_5]]#1 : !quake.wire, !quake.wire
// CHECK:           }
// CHECK:           %[[VAL_6:.*]]:2 = quake.z [%[[VAL_3]]#0] %[[VAL_3]]#1 : (!quake.wire, !quake.wire) -> (!quake.wire, !quake.wire)
// CHECK:           return
// CHECK:         }

