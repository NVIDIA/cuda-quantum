    %c1_i32 = arith.constant 1 : i32
    %true = arith.constant true
    %c0_i32 = arith.constant 0 : i32
    %0 = quake.alloca !quake.ref
    %1 = cc.alloca i32
    cc.store %c0_i32, %1 : !cc.ptr<i32>
    cc.if(%true) {
      quake.x %0 : (!quake.ref) -> ()
      cc.store %c1_i32, %1 : !cc.ptr<i32>
