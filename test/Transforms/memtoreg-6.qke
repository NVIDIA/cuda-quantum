func.func private @live(i32, i32, i32)

func.func @test_3() {
  %0 = cc.alloca i32
  %1 = cc.alloca i32
  %2 = arith.constant 42 : i32
  %c = arith.constant true
  %3 = cc.if (%c) -> i32 {
    %10 = arith.constant 8 : i32
    %11 = arith.constant 14 : i32
    cc.store %10, %0 : !cc.ptr<i32>
    cc.store %11, %1 : !cc.ptr<i32>
    cc.continue %2 : i32
  } else {
    %10 = arith.constant 23 : i32
    %11 = arith.constant 78234 : i32
    cc.store %10, %0 : !cc.ptr<i32>
    cc.store %11, %1 : !cc.ptr<i32>
    cc.continue %2 : i32
  }
  %4 = cc.load %0 : !cc.ptr<i32>
  %5 = cc.load %1 : !cc.ptr<i32>
  call @live(%3, %4, %5) : (i32, i32, i32) -> ()
  return
}

