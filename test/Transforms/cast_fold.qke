// CHECK:   }

func.func @i32_repeat() -> i32 {
  %0 = arith.constant -1: i32
  %1 = cc.cast unsigned %0 : (i32) -> i32
  %2 = cc.cast unsigned %1 : (i32) -> i32
  return %2 : i32
}

