// Cast folding optimizations on integer casts

func.func @i32_nop() -> i32 {
  %0 = arith.constant -1: i32
  %1 = cc.cast %0 : (i32) -> i32
  return %1 : i32
}

// CHECK:   func.func @i32_nop() -> i32 {
// CHECK:     %[[VAL_0:.*]] = arith.constant -1 : i32
