// CHECK:   %6 = arith.addf %5, %cst : f64
// CHECK:   cc.store %6, %3 : !cc.ptr<f64>
// CHECK:   quake.x [%0] %1 : (!quake.ref, !quake.ref) -> ()
// CHECK:   quake.x %0 : (!quake.ref) -> ()
// CHECK:   quake.x %1 : (!quake.ref) -> ()
// CHECK:   quake.x [%0] %1 : (!quake.ref, !quake.ref) -> ()
// CHECK:   %7 = cc.load %3 : !cc.ptr<f64>
