  quake.rz (%cst) %1 : (f64, !quake.ref) -> ()
  quake.x [%0] %1 : (!quake.ref, !quake.ref) -> ()
  quake.x %0 : (!quake.ref) -> ()
  quake.rz (%cst_1) %1 : (f64, !quake.ref) -> ()
  quake.x [%0] %1 : (!quake.ref, !quake.ref) -> ()
  quake.rz (%cst_2) %1 : (f64, !quake.ref) -> ()
  quake.x %1 : (!quake.ref) -> ()
  quake.rz (%cst_3) %1 : (f64, !quake.ref) -> ()
  cc.return
}

